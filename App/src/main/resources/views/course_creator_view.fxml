<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="720.0" prefWidth="1280.0"
            style="-fx-background-color: #0fa000" fx:controller="views.CourseCreatorView">

    <TextField AnchorPane.topAnchor="10.0"
               AnchorPane.leftAnchor="10.0"
               promptText="Nombre"
               focusTraversable="false" fx:id="txtName"/>

    <Button AnchorPane.topAnchor="60.0"
            AnchorPane.leftAnchor="10.0"
            text="registrar"
            onAction="#addCourse" />

    <Button AnchorPane.topAnchor="100.0"
            AnchorPane.leftAnchor="10.0"
            text="regresar"
            onAction="#back" />

</AnchorPane>
